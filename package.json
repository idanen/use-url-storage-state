  "keywords": [
    "react",
    "hook",
    "url",
    "state",
    "query parameters",
    "react-router"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/orduer/use-url-storage-state.git"
